generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id        String   @id @default(cuid())
  type      String
  mint      String   @unique
  name      String
  symbol    String?
  creator   String
  signature String   @unique
  slot      BigInt
  timestamp DateTime

  @@index([mint])
}
